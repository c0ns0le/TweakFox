<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>tweakfox@tweakfox.net</em:id>
    <em:version>1.3</em:version>
    <em:type>2</em:type>
   
    <!-- Target Application this extension can install into, 
         with minimum and maximum supported versions. --> 
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>3.4</em:minVersion>
        <em:maxVersion>99.1.*</em:maxVersion>
      </Description>
    </em:targetApplication>
   
    <!-- Front End MetaData -->
    <em:name>tweakfox</em:name>
    <em:description>Preconfigured addons and preferences for Firefox</em:description>
    <em:creator>Martin Francis O'Connor</em:creator>
    <em:homepageURL>http://www.computing.dcu.ie/~moconnor</em:homepageURL>
  </Description>      
</RDF>